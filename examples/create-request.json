{
    "args": {
        "docker-hosts": [
            "unix:///var/run/docker.sock"
        ],
        "dry-run": true,
        "debug": false,
        "filters": {
            "stop-modes": [
                "labelled",
                "writers",
                "linked"
            ],
            "include-names": [],
            "include-volumes": [],
            "exclude-names": [],
            "exclude-volumes": []
        },
        "backup-id": "20240324_0159",
        "backup-dir": "/tmp/backups",
        "label-prefix": "bckupr",
        "notification-urls": []
    },
    "templates": {
        "backup": {
            "alias": "restore",
            "image": "busybox",
            "cmd": [
                "sh",
                "-c",
                "mkdir -p /backup/{backup_id}; tar czvf /backup/{backup_id}/{name}.tar.gz /data"
            ],
            "env": null,
            "volumes": null
        },
        "file-check": {
            "alias": "file-check",
            "image": "busybox",
            "cmd": [
                "ls",
                "/backup/{backup_id}/{name}.tar.gz"
            ],
            "env": null,
            "volumes": null
        },
        "offsite-push": {
            "alias": "offsite-push",
            "image": "",
            "cmd": null,
            "env": null,
            "volumes": null
        }
    }
}
